<h1>Listing contacts</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Summary</th>
    <th>Url</th>
    <th>Address</th>
    <th>Phone</th>
  </tr>

<% @contacts.each do |contact| %>
  <tr>
    <td><%=h contact.title %></td>
    <td><%=h contact.summary %></td>
    <td><%=h contact.url %></td>
    <td><%=h contact.address %></td>
    <td><%=h contact.phone %></td>
    <!-- id = Long.valueOf(contact.key.getId).toString' -->
                         <% id = '7' %>
    <td><%= link_to 'Show', id %></td>
    <td><%= link_to 'Edit', id + '/edit' %></td>
    <td><%= link_to 'Destroy', "contact_confirm_delete" %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New contact', @params.controller + '/new' %>
